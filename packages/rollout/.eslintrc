{
    "extends": [
        "eslint-config-airbnb-base",
        "plugin:jest/recommended"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": 0,
        "import/prefer-default-export": 0
    },
    "plugins": [
        "import",
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    }
}