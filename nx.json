{
    "extends": "nx/presets/npm.json",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": [
                    "lint",
                    "test:unit",
                    "type-check",
                    "lint:js",
                    "lint:styles",
                    "build:lib"
                ],
                "accessToken": "NmE2NmFlYTUtN2JhZi00ZmQ3LWEyYzMtYWRkNzc3MTg0YmUyfHJlYWQ=",
                "parallel": 5
            }
        }
    },
    "targetDefaults": {
        "build:lib": {
            "dependsOn": ["^build:lib"],
            "outputs": ["./lib", "./build"]
        },
        "type-check": {
            "dependsOn": ["^build:lib"],
            "outputs": [],
            "inputs": [
                "default",
                "{workspaceRoot}/tsconfig.json",
                "{workspaceRoot}/tsconfig.aliases.json",
                "{workspaceRoot}/tsconfig.lib.json"
            ]
        },
        "test:unit": {
            "dependsOn": ["^build:lib"],
            "outputs": [],
            "inputs": [
                "default",
                "{workspaceRoot}/jest.config.base.js",
                "{workspaceRoot}/jest.config.native.js"
            ]
        },
        "lint:js": {
            "inputs": [
                "{projectRoot}/**/*.js",
                "{projectRoot}/**/*.ts",
                "{projectRoot}/**/*.tsx",
                "{workspaceRoot}/.eslintrc.js"
            ]
        },
        "lint:styles": {
            "inputs": ["{projectRoot}/**/*.tsx"]
        }
    },
    "implicitDependencies": {
        "package.json": "*",
        "nx.json": "*"
    },
    "affected": {
        "defaultBase": "origin/develop"
    }
}
