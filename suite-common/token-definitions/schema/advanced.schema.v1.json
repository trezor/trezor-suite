{
    "$id": "https://data.trezor.io/nft-definitions/schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Advanced token definitions, contract addresses together with names and symbols",
    "type": "object",
    "patternProperties": {
        "^[a-zA-Z0-9]+$": {
            "type": "object",
            "properties": {
                "symbol": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": ["symbol", "name"]
        }
    }
}
